{
  "name": "can-dom-events",
  "description": "Listen for native and custom DOM events",
  "version": "0.0.0",
  "author": {
    "name": "Chris Andrejewski",
    "email": "core@donejs.com",
    "url": "https://www.bitovi.com/"
  },
  "bugs": {
    "url": "https://github.com/canjs/can-dom-events/issues"
  },
  "dependencies": {
    "can-namespace": "^1.0.0",
    "can-util": "^3.8.1"
  },
  "devDependencies": {
    "fixpack": "^2.3.1",
    "jshint": "^2.9.1",
    "steal": "^1.3.1",
    "steal-qunit": "^1.0.1",
    "steal-tools": "^1.2.0",
    "testee": "^0.6.0"
  },
  "files": [
    "src",
    "dist"
  ],
  "homepage": "https://github.com/canjs/can-dom-events",
  "keywords": [
    "canjs",
    "custom",
    "dom",
    "events",
    "listener"
  ],
  "license": "MIT",
  "main": "dist/cjs/can-dom-events",
  "repository": {
    "type": "git",
    "url": "git://github.com/canjs/can-dom-events.git"
  },
  "scripts": {
    "build": "node build.js",
    "jshint": "jshint ./*.js ./helpers/*.js --config",
    "lint": "fixpack && npm run jshint",
    "postpublish": "git push --tags && git checkout master && git branch -D release && git push",
    "preversion": "npm test && npm run build",
    "release:major": "npm version major && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:patch": "npm version patch && npm publish",
    "test": "npm run lint && npm run testee",
    "testee": "testee test.html --browsers firefox",
    "version": "git commit -am \"Update version number\" && git checkout -b release && git add -f dist/"
  },
  "steal": {
    "main": "can-dom-events",
    "configDependencies": [
      "live-reload"
    ],
    "npmIgnore": [
      "testee",
      "steal-tools"
    ]
  }
}
